// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "webpack-dev",
			"problemMatcher": {
				"owner": "webpack",
				"background": {
					"beginsPattern": "^Issues checking in progress...",
					"endsPattern": "^(Hash: \\w+|No issues found.)"
				},
				"pattern": [
					{
						"regexp": "^(ERROR|WARNING) in (.*):(\\d+):(\\d+)\\n(.+)$",
						"severity": 1,
						"file": 2,
						"line": 3,
						"column": 4,
						"message": 5
					}
				]
			},
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}